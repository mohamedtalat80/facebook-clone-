{
    
    "builds": [  
        {
            "src": "usermodel/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" ,"runtime":"python3.9.9"}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "usermodel/wsgi.py"
        }
    ]
}