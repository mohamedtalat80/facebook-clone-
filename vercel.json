{
    
    "builds": [  
        {
            "src": "user_model/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" ,"runtime":"python3.9.9"}
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "user_model/wsgi.py"
        }
    ]
}
